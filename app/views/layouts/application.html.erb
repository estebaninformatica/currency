<!DOCTYPE html>
<html>
<head>
  <title>Paridad Cambiaria</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <nav class="navbar navbar-default ">
    <div class="container-fluid">
      <div class="navbar-header">
        <%= link_to 'MONEDAS', currencies_path ,{:class=> "navbar-brand"} %>
        <%= link_to 'TIPOS DE CAMBIOS', type_changes_path ,{:class=> "navbar-brand"} %>
        <%= link_to 'CAMBIOS', changes_path ,{:class=> "navbar-brand"} %>
        <%= link_to 'HISTORIAL DE CAMBIOS', change_historicals_path ,{:class=> "navbar-brand"} %>
      </div>
    </div>
  </nav>
  <div class="container-fluid">
    <%= yield %>
  </div> 

</body>
</html>
